{
  "name": "devscard",
  "description": "Template for creating a comprehensive virtual CV for developers.",
  "version": "0.0.0-g5b45e90314",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "postinstall": "npm run generate-favicons",
    "dev": "astro dev",
    "prebuild": "move-file ./src/pages/pdf.astro ./src/pages/_pdf.astro && npm run generate-favicons",
    "build": "astro build",
    "postbuild": "move-file ./src/pages/_pdf.astro ./src/pages/pdf.astro",
    "preview": "astro preview",
    "generate-pdf": "ts-node scripts/generate-pdf.ts",
    "generate-favicons": "ts-node scripts/generate-favicons.ts",
    "prettier:check": "prettier --check . --config .prettierrc.yaml",
    "prettier:write": "prettier --write . --config .prettierrc.yaml",
    "astro:check": "astro check",
    "ts:check": "tsc --jsx preserve --skipLibCheck",
    "check": "concurrently npm:*:check",
    "prepare": "husky"
  },
  "dependencies": {
    "@floating-ui/dom": "1.7.4",
    "auto-changelog": "2.5.0",
    "broken-link-checker": "0.7.8",
    "iconify-icon": "1.0.8",
    "nanoid": "4.0.2"
  },
  "devDependencies": {
    "@astrojs/image": "0.18.0",
    "@astrojs/react": "2.3.2",
    "@astrojs/tailwind": "4.0.0",
    "@lhci/cli": "0.15.1",
    "@percy/cli": "1.31.3",
    "@types/marked": "5.0.2",
    "astro": "2.10.15",
    "astro-compress": "2.3.8",
    "concurrently": "8.2.2",
    "cspell": "8.19.4",
    "czg": "1.12.0",
    "date-fns": "2.30.0",
    "favicons": "7.2.0",
    "husky": "9.1.7",
    "iconify-icon-names": "1.1.0",
    "immer": "10.1.3",
    "locales-ts": "1.0.0",
    "marked": "5.1.2",
    "move-file-cli": "3.0.0",
    "nerdbank-gitversioning": "3.8.118",
    "photoswipe": "5.4.4",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "puppeteer": "22.15.0",
    "puppeteer-report": "3.2.0",
    "rollup-plugin-visualizer": "5.14.0",
    "tailwindcss": "3.4.18",
    "ts-node": "10.9.2",
    "type-fest": "3.13.1",
    "typescript": "5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KonradSzwarc/devscard.git"
